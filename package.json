{
    "name": "@fmented/scp",
    "description": "A simple svelte component parser to get necessary info for documentation",
    "repository": {
        "type": "git",
        "url": "https://github.com/fmented/scp/"
    },
    "homepage": "https://github.com/fmented/scp/",
    "author": "fauky",
    "version": "0.2.2",
    "license": "MIT",
    "main": "dist/index.js",
    "typings": "dist/types.d.ts",
    "engines": {
        "node": ">=10"
    },
    "scripts": {
        "start": "tsdx watch",
        "build": "tsdx build",
        "test": "tsdx test",
        "lint": "tsdx lint",
        "prepare": "tsdx build"
    },
    "husky": {
