        "tslib": "^2.3.1",
        "typescript": "^3.9.10"
    },
    "bin": {
        "scpd": "./bin/scp"
    },
    "keywords": [
        "scp",
        "svelte",
        "parser",
        "documentation",
        "utility",
        "cli-tool"
